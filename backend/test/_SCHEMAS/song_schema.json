{
    "title": "Song",
    "name": "song_schema",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "default": "Untitled"
        },
        "runtime": {
            "type": "integer",
            "default": 200
        },
        "artists": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "default": "Artist"
                    }
                }
            }
        }
    }
}